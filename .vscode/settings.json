{
  "todo-tree.rootFolder": "src",
  "todo-tree.defaultHighlight": {
    "icon": "clock",
    "type": "text",
    "foreground": "#37B942",
    "background": "#002211",
    "opacity": 30,
  },
  "emmet.optimizeStylesheetParsing": true,
  "emmet.includeLanguages": {
    "ejs": "html",
  },
  "beautify.language": {
    "html": [
      "ejs",
      "html"
    ]
  },
  "html.format.endWithNewline": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "html.format.wrapLineLength": 180,
  "cSpell.allowCompoundWords": true,
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "sass/@gov.au/"
  ],
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.logLevel": "Information",
  "cSpell.words": [
    "uikit",
    "markdown",
    "latex",
    "plaintext",
    "html",
    "javascript",
    "scss",
    "json",
    "refactor",
    "topbar",
    "justclaims",
    "ndash",
    "incap",
    "NLHC",
    "deleteclaim",
    "deleteclaimmodal",
    "withdrawclaim",
    "withdrawclaimmodal",
    "noclaims",
    "fullwidth",
    "claimdetail",
    "Suser",
    "Quser",
    "Puser",
    "Cuser",
    "speakerbox",
    "tabindex",
    "Faroe",
    "Tajikstan",
    "Warooka",
    "Tajikstan",
    "Townsville",
    "Anglicare",
    "Anlgi",
    "Coutts",
    "Redington",
    "Slater",
    "myaccount",
    "markup",
    "reusables",
    "myservice",
    "DVA",
    "DVA's",
    "mygov",
    "muuri",
    "xmlns",
    "svguri",
    "xlink",
    "unauth",
    "nomrep",
    "spondylosis",
    "keratosis",
    "MRCA"
  ],
  "emmet.extensionsPath": "./.vscode/emmet/",
  "emmet.triggerExpansionOnTab": true,
  "emmet.showSuggestionsAsSnippets": true,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "clock",
    "type": "text",
    "foreground": "#37B942",
    "background": "#002211",
    "opacity": 30
  },
  "todo-tree.general.rootFolder": "src"
}