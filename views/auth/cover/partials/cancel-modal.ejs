<div role="dialog" aria-hidden="true" aria-labelledby="cancelModalTitle" aria-describedby="cancelModalDesc" id="open-cancel-modal" class="modal-window">
	<div role="document">
		<a href="#" title="Close" class="modal-close" id="cancelModalCloseBtn">Close</a>
		<h1 id="cancelModalTitle">Do you want to cancel?</h1>
		<p id="cancelModalDesc">This will delete your claim and you will need to start over.</p>
		<div class="modal-buttons">
			<button id="cancelModalNoBtn" class="uikit-btn floated uikit-btn--tertiary" onclick="window.location.href = '#'">No</button>
			<button id="cancelModalYesBtn" class="uikit-btn floated " onclick="window.location.href = '/auth'">Yes</button>
		</div>
	</div>
</div>

<%
	var modalName = "open-cancel-modal";
	var triggerName = "coverCancelBtn";
	var modalElementPrefix = "cancelModal";
%>
<% include modal-common.ejs %>
