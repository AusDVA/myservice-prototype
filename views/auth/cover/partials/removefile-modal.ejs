<div role="dialog" aria-hidden="true" aria-labelledby="removeFileModalTitle" aria-describedby="removeFileModalDesc" id="open-removefile-modal" class="modal-window">
	<div role="document">
		<a href="#" title="Close" class="modal-close" id="removeFileModalCloseBtn">Close</a>
		<h3 id="removeFileModalTitle">Do you want to remove the file?</h3>
		<div id="removeFileModalDesc">Medical documents help us to assess your claim.</div>
		<div class="modal-buttons">
			<button id="removeFileModalNoBtn" class="uikit-btn floated uikit-btn--tertiary" onclick="window.location.href = '#'">No</button>
			<button id="removeFileModalYesBtn" class="uikit-btn floated" onclick="window.location.href = '#'">Yes</button>
		</div>
	</div>
</div>

<%
	var modalName = "open-removefile-modal";
	var triggerName = "coverRemoveFileBtn";
	var modalElementPrefix = "removeFileModal";
%>
<% include modal-common.ejs %>
