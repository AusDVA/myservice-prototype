<%- include(templates+"header", {
  claim: "Commonwealth Seniors Health Card",
  heading: "Dependent children"
}) %>

<div class="box boxed" [ngClass]="getDottedDeleteClass(incomeAsset)"
  *ngFor="let incomeAsset of incomeAssets[currentIncomeAssetsType.key]">
  <div class="box__header">
    <h2 class="box__heading" [ngClass]="getDeleteClass(incomeAsset)">
      <fa-icon [classes]="['fa-lg']" [icon]="['fal', currentIncomeAssetsType.icon]"></fa-icon>
      <span class="font-weight-heavy">
        Gifts test</span>
    </h2>
    <%- include(partials+"components/tooltip-global", {
      text: 'You cannot change a deprived asset. In normal circumstances it will continue to be assessed for a period of 5 years. If you have any queries about this item, or you have now received some consideration for the transfer of this asset, please contact DVA on 133 254.',
      customClass: 'tooltip--review-item'
      }) %>
  </div>
  <div class="box__table" [ngClass]="getDeleteClass(incomeAsset)">
    <div class="tr">
      <div class="td">
        <span class="font-weight-heavy">
          Testing overflow
        </span>
      </div>
      <div class="td">
        testing data
      </div>
    </div>
  </div>
  <ng-container *ngIf="incomeAsset.isEdited || incomeAsset.isDeleted || !incomeAsset.isPrepop">
    <div class="status-label margin-above" [ngClass]="getFlagClass(incomeAsset)">
      {{ getFlagText(incomeAsset) }}
    </div>
  </ng-container>
</div>

<%- include(templates+"footer") %>