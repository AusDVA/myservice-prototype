<!DOCTYPE html>
<html class="js" lang="en">

<head>
  <title>Register: Personal details | MyService</title>
<%- include(partials+"head") %>
</head>

<body class="uikit-body uikit-grid uikit-refactor">
  <noscript>
    <p role="alert">This website needs JavaScript to work properly.</p>
  </noscript>
  <nav class="uikit-skip-link">
    <a class="uikit-skip-link__link" href="#main-content">Skip to main content</a>
  </nav>
  <%- include(partials+"header-topbar") %>
<%- include(partials+"header-nom-rep") %>
  <main>
    <div id="main-content" class="section-feature border-bottom--mild margin-above--none margin-below--none padding-below">
      <div class="container margin-above--none">
        <h1 class="margin-above--none ss-align--center">
          <span class="heading">Register</span>
        </h1>
      </div>
    </div>
    <div class="container">
      <div class="form-group margin-above--large">
          <div class="row">
            <div class="col-sm-12">
              <label class="uikit-text-input__label" for="Input field">PMKeyS <span class="hint">(optional)</span></label>
                <input class="uikit-text-input input-half" name="Input field" id="Input field" type="text" value="" aria-required="true">
                <label class="uikit-text-input__label" for="Input field">Service number <span class="hint">(optional)</span></label>
                  <input class="uikit-text-input input-half" name="Input field" id="Input field" type="text" value="" aria-required="true">
              <fieldset class="margin-above--large">
                <legend>Contact details</legend><br />

                <label class="uikit-text-input__label" for="Input field">Email address</label>
                  <input class="uikit-text-input input-half" name="Input field" id="Input field" type="text" value="" aria-required="true">
                  <label class="uikit-text-input__label" for="Input field">Mobile phone number <span class="hint">(preferred)</span></label>
                    <input class="uikit-text-input input-half" name="Input field" id="Input field" type="text" value="" aria-required="true">
                    <label class="uikit-text-input__label" for="Input field">Home phone number <span class="hint">(optional)</span></label>
                      <input class="uikit-text-input input-half" name="Input field" id="Input field" type="text" value="" aria-required="true">

                <legend class="margin-above--mid">Postal address </legend>
                <%- include(partials+"address-postal") %>
                  <br>
                  <legend>Residential address</legend>
                  <div class="form-group">
                    <label class="uikit-control-input">
                      <input class="uikit-control-input__input sameAsPostal" type="checkbox" name="sameAsPostal" value="" checked>
                      <span class="uikit-control-input__text">Same as postal address</span>
                    </label>
                  </div>
                  <div class="pt-showIfAdditionalAddress">
                    <%- include(partials+"address-residential") %>
                  </div>
                  <div class="pagination">
                    <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/auth/profile/nomrep/veteran-details'">Previous</button>
                    <button class="uikit-btn floated btnNext" onclick="window.location.href = '/auth/profile/nomrep/role-rep'">Next</button>
                  </div>

            </div>

          </div>

        </div>
      </main>
        <%- include(partials+"footer") %>
          <script src="/docs/js/jquery-effects.js"></script>
          <script src="/docs/js/main.js"></script>
          <script src="/docs/js/jquery-3.2.1.min.js"></script>
          <script>
            // Progress percentage
            document.getElementById("percentage").style.width = "50%";
            document.getElementById("percentage_value").textContent = "50%";

          </script>
</body>

</html>
