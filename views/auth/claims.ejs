          <!-- <div class="row"> -->
            <!-- <div class="col-md-offset-2 col-md-8"> -->
              <div class="back-button">
                <a href="/auth"><i class="fal fa-arrow-left fa-fw"></i>Back to home</a>
              </div>
              <h1 class="margin-above--none margin-below--mid">Claims and applications</h1>


     
      <section class="dashboard-cards">
        <h2 class="dashboard-title">Group heading</h2>
        <div class="cards__wrapper" id="claims-tiles">
          <% cards.forEach(function(card) { %>
          <%- include(partials+"components/card-v2", {
            id: card.id,
            title: card.title,
            text: card.text,
            link: card.link
          }) %>
          <% }); %>
        </div>
        <h2 class="dashboard-title">Another group heading</h2>
        <div class="cards__wrapper" id="claims-tiles">
          <% cards2.forEach(function(card) { %>
          <%- include(partials+"components/card-v2", {
            id: card.id,
            title: card.title,
            text: card.text,
            link: card.link
          }) %>
          <% }); %>
        </div>
      </section>
     </div>
  </main1>
  <%- include(partials+"components/layout/footer") %>
</body>
<%- include("index-state-switcher") %>

