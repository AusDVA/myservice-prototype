<%
  var progress = JSON.parse(include("b-progressbar")) 
  progress.activeItem = 3;
%>

<%- include(templates+"header", { 
  claim: "MyService - Income support supplement", 
  heading: "Residency",
  progress
}) %>

<%- include(partials+"components/form-partials/yes_no", {
  label: "Are you an Australian citizen?"
}); %>

<%- include(partials+"components/form-partials/checkbox", {
  label: "Are you:",
  boxes: [
    "The holder of a permanent visa",
    "The holder of a special purpose visa",
    "The holder of a special category visa",
    "A refugee or former refugee"
  ]
}); %>

<%- include(partials+"components/form-partials/notice", {
  message: "You will need to provide a copy of your visa or special category visa that shows you hold the ability to remain in Australia."
}); %>

<%- include(partials+"components/form-partials/yes_no", {
  label: "Are you living permanently in Australia?"
}); %>

<%- include(partials+"components/form-partials/notice", {
  message: "You must be living permanently in Australia at the time of lodging your claim for Service Pension.",
  type: "warning"
}); %>

<%- include(partials+"components/form-partials/yes_no", {
  label: "Were you born in Australia?"
}); %>

<%- include(partials+"components/form-partials/dropdown", {
  label: "What is your country of birth?",
  options: [
    "Mr",
    "Mrs",
    "Master",
    "Miss",
    "Dr"
  ]
}); %>

<%- include(partials+"components/form-partials/date", {
  label: "When did you first arrive in Australia?",
  hint: "dd / mm / yyyy"
}); %>

<%- include(partials+"components/form-partials/yes_no", {
  label: "Have you lived overseas and returned to live in Australia in the last 12 months?",
  all_toggle: "#save-next"
}); %>

<%- include(partials+"components/form-partials/yes_no", {
  label: "Have you moved to or returned to live in Australia in the last 12 months?",
  all_toggle: "#save-next"
}); %>



