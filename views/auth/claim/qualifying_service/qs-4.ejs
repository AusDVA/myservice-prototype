<!DOCTYPE html>
<html class="js" lang="en">

<head>
  <meta charset="utf-8" />
  <meta content="width=device-width" name="viewport" />
  <title>Review and submit: Service History | MyService</title>
  <%- include(partials+"components/layout/head") %>
</head>

<body class="uikit-body uikit-grid uikit-refactor">
  <noscript>
    <p role="alert">This website needs JavaScript to work properly.</p>
  </noscript>
  <nav class="uikit-skip-link">
    <a class="uikit-skip-link__link" href="#main-content">Skip to main content</a>
  </nav>
  <%- include(partials+"components/layout/header-topbar") %>
  <%- include(partials+"components/layout/header-nom-rep") %>
  <main>
    <div id="main-content" class="section-feature border-bottom--mild margin-above--none margin-below--none padding-below">
      <h1 class="margin-above--none ss-align--center">
        <span class="heading">Review and submit</span>
      </h1>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-md-8 col-md-offset-2">

<%- include(components+"review-submit", {
  claim_title: "Qualifying service",
  modifiers: ["marginAbove", "marginBelow", "claimDetails"],
  categories: [
    {
      title: "Service",
      edit_url: "/auth/claim/qualifying_service/qs-1",
      data: [
        {
          q: "Type of claim",
          a: "Australian Defence Force"
        },
        {
          q: "Former names",
          a: `<ul>
                <li>Joseph Smith</li>
                <li>Jo Smith</li>
              </ul>`
        },
        {
          q: "Partner name",
          a: "Mrs Joe Bloggs"
        }
      ]
    },
    {
      title: "Service History",
      edit_url: "/auth/claim/qualifying_service/qs-2",
      data: [
        "Service",
        {
          q: "Service number",
          a: "123 456 789 K"
        },
        {
          q: "Type of service",
          a: "Permanent"
        },
        {
          q: "Date of enlistment",
          a: "21 / 09 / 1945"
        },
        {
          q: "Date of discharge",
          a: "21 / 06 / 1949"
        },
        {
          q: "Branch of service",
          a: "Navy"
        },
        "Operation",
        {
          q: "Conflict name",
          a: "Afghanistan"
        },
        {
          q: "Operation name",
          a: "Op Slipper"
        },
        {
          q: "Start date",
          a: "21 / 09 / 1945"
        },
        {
          q: "End date",
          a: "21 / 06 / 1949"
        }
      ]
    }
  ]
}) %>
  

          <div class="pagination margin-above--extra">
            <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/auth/claim/qualifying_service/qs-3'">Previous</button>
            <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '#open-modal'">Cancel application</button>
            <div class="floated">
              <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/auth/claim'">Save and exit</button>
              <button class="uikit-btn btnNext" id="btnNext" onclick="window.location.href = '/auth/claim/qualifying_service/qs-5'">Submit</button>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%- include(partials+"content/modals/claims-cancel-modal") %>
  </main>
  <%- include(partials+"components/layout/footer") %>
</body>

<script>
  $(document).ready(function () {

    if (sessionStorage.getItem('commonwealth-allied') != 'yes') {
      $("#adf").show();
    } else {
      $("#commonwealth").show();
    }
  });
</script>

</html>
