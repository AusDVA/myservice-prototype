<!DOCTYPE html>
<html class="js" lang="en">

<head>
  <title>Submitted: DHOAS Application for a Subsidy Certificate | MyService</title>
<% include ../../../../partials/head.ejs %>
</head>

<body class="uikit-body uikit-grid uikit-refactor">
  <noscript>
    <p role="alert">This website needs JavaScript to work properly.</p>
  </noscript>
  <nav class="uikit-skip-link">
    <a class="uikit-skip-link__link" href="#main-content">Skip to main content</a>
  </nav>
  <% include ../../../../partials/header-topbar.ejs %>
  <% include ../../../../partials/header-claims.ejs %>
  <% include ../../../../partials/header-nom-rep.ejs %>

  <main>
    <div class="container" id="main-content">
      <div class="row">
        <div class="col-md-offset-1 col-md-10">
          <div class="claim-end">

            <div class="pt-claim-type--normal">
              <div class="margin-above ie-success-animation">
                <img src="../../../docs/images/ico-tick-green-outline.svg" alt="Success tick icon">
              </div>
              <div class="margin-above submitted-animation">
                <% include ../../../../partials/animation-submitted.ejs %>
              </div>
              <div class="cantered-item transition-content transition-content--open pt-claim-animation-start">
                <h1>We're submitting your application for a Subsidy Authorisation Request</h1>
              </div>
            </div>

            <div class="transition-content pt-claim-animation-end margin-below--extra">
              <div class="pt-claim-type--normal">
                <h1 class="cantered-item">Your application for a Subsidy Authorisation Request has&nbsp;been
                  <span class="font-weight-heavy"> submitted</span>
                </h1>
              </div>

              <hr class="orange-divider">

              <div class="margin-below--extra">
                <h3>You will receive an outcome by email within 5 business days.</h3>
                <p class="margin-below--none">If you do not hear from us within this time you can contact us on
                  <a href="tel:1300434627">1300 434 627</a>.</p>
                <br>
                <p>For more information about this scheme, visit the
                  <a href="http://www.dhoas.gov.au" rel="external">DHOAS website</a>.
                </p>
              </div>

              <div class="pagination">
                <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/auth/profile'">Update profile</button>
                <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/auth/claim'">See your claims</button>
              </div>

            </div>
          </div>
        </div>
      </div>
    </div>
  </main>

  <% include ../../../../partials/footer.ejs %>


  <script>
    var timeDelay = 3000;
    setTimeout(
      function () {
        $(".pt-claim-animation-start").toggleClass("transition-content--open");
        $(".pt-claim-animation-end").toggleClass("transition-content--open");
      }, timeDelay);
  </script>

</body>

</html>