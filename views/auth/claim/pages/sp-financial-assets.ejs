<!DOCTYPE html>
<html class="js" lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width" name="viewport">
  <title>Service Pension - Financial assets | MyService</title>
  <link href="docs/css/main.css" rel="stylesheet">
</head>

<body class="uikit-body uikit-grid uikit-refactor">
  <noscript>
    <p role="alert">This website needs JavaScript to work properly.</p>
  </noscript>
  <nav class="uikit-skip-link">
    <a class="uikit-skip-link__link" href="#main-content">Skip to main content</a>
  </nav>
  <% include ../../../global/partials/header-topbar-new.ejs %>

    <main>
      <div id="main-content" class="section-feature border-bottom--mild margin-above--none margin-below--none padding-below">
        <div class="container margin-above--none">
          <!-- Progress bar -->
          <div class="col-md-12">
            <nav>
              <ul class="progress-container">
                <li class="progress-step">
                  <a href="sp-financial-details">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Financial details</p>
                </li>
                <li class="progress-step progress-step--active">
                  <a href="sp-financial-assets">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Financial assets</p>
                  </a>
                </li>
                <li class="progress-step ongoing-only">
                  <a href="sp-personal-assets">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Personal assets</p>
                  </a>
                </li>
                <li class="progress-step ongoing-only">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">Income & payments</p>
                </li>
                <li class="progress-step">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">Other items</p>
                </li>
                <li class="progress-step progress-step--last">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">Superannuation</p>
                </li>
              </ul>
            </nav>
          </div>
        </div>

        <!-- Page heading -->
        <h1 class="margin-above--none ss-align--center">
          <span class="heading">Financial assets</span>
        </h1>
      </div>

      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <!-- Page content -->
            <h2 class="margin-above--large">
              <span class="font-weight-heavy">Financial assets information</span>
            </h2>

            <!-- bottom page navigation -->
            <div class="pagination">
              <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/sp-financial-details'">Previous</button>
              <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '#open-modal'">Cancel claim</button>
              <div class="floated">
                <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/claims'">Save and exit</button>
                <button class="uikit-btn btnNext" onclick="window.location.href = '/sp-personal-assets'">Save and next</button>
              </div>
            </div>
            <% include ../partials/cancel-modal.ejs %>
          </div> <!-- col-md-12 end -->
        </div> <!-- row end -->
      </div> <!-- container end -->

    </main>

    <% include ../../../global/partials/footer.ejs %>


      <script>
        // Progress percentage
        document.getElementById("percentage ").style.width = "20% ";
        document.getElementById("percentage_value ").textContent = "20% ";
      </script>


</body>

</html>