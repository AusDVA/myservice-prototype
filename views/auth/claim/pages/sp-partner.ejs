<!DOCTYPE html>
<html class="js" lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width" name="viewport">
    <title>Service Pension - Partner | MyService</title>
    <link href="docs/css/main.css" rel="stylesheet">
</head>

<body class="uikit-body uikit-grid uikit-refactor"></body>
<noscript>
    <p role="alert">This website needs JavaScript to work properly.</p>
</noscript>
<nav class="uikit-skip-link">
    <a class="uikit-skip-link__link" href="#main-content">Skip to main content</a>
</nav>
<% include ../../../global/partials/header-topbar-new.ejs %>
<main>
    <div id="main-content" class="section-feature border-bottom--mild margin-above--none margin-below--none padding-below">
        <div class="container margin-above--none">
            <!-- Progress bar -->
            <div class="col-md-12">
                <nav>
                    <ul class="progress-container">
                        <li class="progress-step progress-step--done">
                            <a href="sp-my-details">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">My details</p>
                </a>
                        </li>
                        <li class="progress-step progress-step--active">
                            <a href="sp-partner">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">Partner</p>
                </a>
                        </li>
                        <li class="progress-step">
                            <a href="sp-dependant">
                  <span class="progress-step__icon"></span>
                  <p class="progress-step-label">Dependent children</p>
                </a>
                        </li>
                        <li class="progress-step">
                            <span class="progress-step__icon"></span>
                            <p class="progress-step-label">Residency</p>
                        </li>
                        <li class="progress-step">
                            <span class="progress-step__icon"></span>
                            <p class="progress-step-label">Living arrangements</p>
                        </li>
                        <li class="progress-step">
                            <span class="progress-step__icon"></span>
                            <p class="progress-step-label">Claim Representative</p>
                        </li>
                        <li class="progress-step progress-step--last">
                            <span class="progress-step__icon"></span>
                            <p class="progress-step-label">Payment details</p>
                        </li>
                    </ul>
                </nav>
            </div>
        </div>
        <!-- Page heading -->
        <h1 class="margin-above--none ss-align--center">
        <span class="heading">Partner</span>
      </h1>
    </div> <!-- container end -->
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="margin-above--extra margin-below--extra">
                    <!-- Page content -->
                    <!-- current relationship status questions -->
                    <fieldset id="relationship-status">
                        <div class="form-group">
                            <label class="uikit-text-input__label" for="radio-buttons">What is your CURRENT relationship status?</label>
                            <p class="uiToolKitCheckBox margin-above">
                                <label class="uikit-control-input">
                                    <input id="single" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Single</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="married" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Married and currently living together</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="defacto" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">In a de facto relationship and currently living together</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="partner-not-living" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Have a partner, but unable to live together because of ill health or infirmity</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="divorced" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Divorced</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="widowed" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Widowed</span>
                                </label>
                            </p>
                            <p class="uiToolKitCheckBox">
                                <label class="uikit-control-input">
                                    <input id="married-separated" class="uikit-control-input__input" type="radio" name="radio-ex">
                                    <span class="uikit-control-input__text">Separated</span>
                                </label>
                            </p>
                        </div>
                    </fieldset>
                    <!-- A -->
                    <div class="form-group" id="date-marriage">
                        <fieldset>
                            <label class="uikit-text-input__label">Date of marriage
                                <span class="hint">(dd / mm / yyyy)</span>
                            </label>
                            <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                            <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                            <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                        </fieldset>
                        <div class="uikit-page-alerts" role="alert" id="A1">
                            <p>You will need to attach a copy of your <strong>marriage certificate</strong> in the documents section </p>
                        </div>
                    </div>
                    <!-- B -->
                    <div class="form-group" id="date-living-together">
                        <fieldset>
                            <label class="uikit-text-input__label">Date commenced living together
                                <span class="hint">(dd / mm / yyyy)</span>
                            </label>
                            <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                            <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                            <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                        </fieldset>
                        <div class="uikit-page-alerts" role="alert" id="A1">
                            <p>If registered you may attach a copy of the <strong>registration certificate</strong> in the documents section</p>
                        </div>
                    </div>
                    <!-- C -->
                    <div class="form-group" id="date-not-together">
                        <fieldset>
                            <label class="uikit-text-input__label">Period unable to live together
                                <span class="hint">(dd / mm / yyyy)</span>
                            </label>
                            <span>From </span><input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                            <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                            <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year"> to
                            <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                            <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                            <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                            <div class="margin-below--mid">
                                <label class="uikit-control-input">
                                    <input class="uikit-control-input__input" type="checkbox" name="or-indefinite" id="or-indefinite">
                                    <span class="uikit-control-input__text">OR indefinite</span>
                                </label>
                        </fieldset>
                    </div>
                    <!-- D -->
                    <div id="divorced-date">
                        <fieldset>
                            <div class="form-group">
                                <label class="uikit-text-input__label">Date of divorce
                                    <span class="hint">(dd / mm / yyyy)</span>
                                </label>
                                <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                                <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                                <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                            </div>
                        </fieldset>
                    </div>
                    <!-- E -->
                    <div class="form-group" id="deceased-partner">
                        <label class="uikit-text-input__label" for="tags">Name of deceased partner</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                        <fieldset>
                            <div class="form-group" id="date-partners-death">
                                <label class="uikit-text-input__label">Date of partner’s death
                                    <span class="hint">(dd / mm / yyyy)</span>
                                </label>
                                <input class="uikit-text-input dd" type="text" name="deceasedStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                                <input class="uikit-text-input mm" type="text" name="deceasedStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                                <input class="uikit-text-input yyyy" type="text" name="deceasedStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                        </fieldset>
                    </div>
                    <!-- F -->
                    <fieldset>
                        <div class="form-group" id="date-separation">
                            <label class="uikit-text-input__label">Date of separation
                                <span class="hint">(dd / mm / yyyy)</span>
                            </label>
                            <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                            <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                            <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                            <div class="uikit-page-alerts" role="alert">
                                <p>You will need to attach a <strong>Separation from Partner form</strong> </p>
                            </div>
                        </div>
                    </fieldset>
                    <!-- G -->
                    <div class="form-group" id="partner-details">
                        <h2>Partner details</h2>
                        <div class="title-field">Title</div>
                        <select class="uikit-text-input title-width" name="title" id="title" type="text" value="">
                            <option>-- Select title --</option>
                            <option selected="">Not specified</option>
                            <option>Miss</option>
                            <option>Mr</option>
                            <option>Mrs</option>
                            <option>Ms</option>
                            <option>Other</option>
                        </select>
                        <div class="form-group">
                            <label class="uikit-text-input__label" for="tags">Family name</label>
                            <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                        </div>
                        <div class="form-group">
                            <label class="uikit-text-input__label" for="tags">Given name(s)</label>
                            <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                        </div>
                        <div class="uikit-text-input__label-wrapper--inline-block">
                            <legend class="uikit-text-input__label legend-label" id="question_gender">Has your partner ever used or been known by other names?
                                <span class="hint display-block margin-below--none">For example, name at birth, maiden name, previous married name</span>
                            </legend>
                        </div>
                        <div class="radio-toggle">
                            <label class="on label-left" for="other-names-yes">
                                <input id="other-names-yes" name="other-names" type="radio">
                                <span>Yes</span>
                            </label>
                            <label class="off label-right" for="other-names-no">
                                <input id="other-names-no" name="other-names" type="radio">
                                <span>No</span>
                            </label>
                        </div>
                        <div class="form-group margin-above" id="sp-partner-formerName">
                            <label class="uikit-text-input__label" for="tags">What was your partner's former name?</label>
                            <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                        </div>
                        <!-- <fieldset class="margin-above">
                            <legend class=" legend-label">Gender</legend>
                            <div class="radio-toggle">
                                <label class="on label-left" for="gender-yes">
                                    <input id="gender-yes" type="radio" name="gender-names">
                                    <span>Female</span>
                                </label>
                                <label class="off label-right" for="gender-no">
                                    <input id="gender-no" type="radio" name="gender-names">
                                    <span>Male</span>
                                </label>
                            </div>
                        </fieldset> -->
                        <fieldset>
                            <div class="form-group margin-above--large">
                                <label class="uikit-text-input__label">Partner’s date of birth
                                    <span class="hint">(dd / mm / yyyy)</span>
                                </label>
                                <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                                <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                                <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                            </div>
                        </fieldset>
                        <!-- <fieldset> -->
                        <legend class="legend-label">Is your partner’s home address the same as your own?</legend>
                        <div class="radio-toggle">
                            <label class="on label-left" for="sp-partner-homeAddress-yes">
                                <input id="sp-partner-homeAddress-yes" type="radio" name="sp-partner-homeAddress">
                                <span>Yes</span>
                            </label>
                            <label class="off label-right" for="sp-partner-homeAddress-no">
                                <input id="sp-partner-homeAddress-no" type="radio" name="sp-partner-homeAddress">
                                <span>No</span>
                            </label>
                        </div>
                        <div class="form-group" id="partners-home-address">
                            <fieldset>
                                <% include ../../../global/partials/address-postal.ejs %>
                            </fieldset>
                        </div>
                        <legend class="legend-label margin-above--large">Is your partner’s postal address the same as your own?</legend>
                        <div class=" radio-toggle">
                            <label class="on label-left" for="sp-partner-postalAddress-yes">
                                <input id="sp-partner-postalAddress-yes" type="radio" name="sp-partner-postalAddress">
                                <span>Yes</span>
                            </label>
                            <label class="off label-right" for="sp-partner-postalAddress-no">
                                <input id="sp-partner-postalAddress-no" type="radio" name="sp-partner-postalAddress">
                                <span>No</span>
                            </label>
                        </div>
                        <div class="form-group" id="partners-postal-address">
                            <fieldset>
                                <% include ../../../global/partials/address-postal.ejs %>
                            </fieldset>
                        </div>
                        <div id="sp-partner-applying">
                            <legend class="legend-label margin-above--large">Does your partner currently hold a Commonwealth Seniors Health Card from Centrelink?</legend>
                            <div class="radio-toggle">
                                <label class="on label-left" for="partner-csh-yes">
                                    <input id="partner-csh-yes" type="radio" name="partner-csh">
                                    <span>Yes</span>
                                </label>
                                <label class="off label-right" for="partner-csh-no">
                                    <input id="partner-csh-no" type="radio" name="partner-csh">
                                    <span>No</span>
                                </label>
                            </div>
                        </div>
                    </div>
                    <!-- [H] CSHC -->
                    <div id="partner-cshc-details">
                        <label class="uikit-text-input__label margin-above" for="crn">What is the Customer Reference Number (CRN) on the card?</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="" style="max-width: 22em;">
                    </div>
                    <div id="partner-centrelink-details">
                        <legend class="legend-label margin-above--large">Is your partner receiving (or applying for) a payment from Centrelink?
                            <span class="hint display-block margin-below--none">This includes for example, age pension, disability support pension, ABSTUDY, payments under the New Enterprise Incentive Scheme (NEIS)</span>
                        </legend>
                        <div class="radio-toggle">
                            <label class="on label-left" for="partner-centrelink-yes">
                                <input id="partner-centrelink-yes" type="radio" name="partner-centrelink">
                                <span>Yes</span>
                            </label>
                            <label class="off label-right" for="partner-centrelink-no">
                                <input id="partner-centrelink-no" type="radio" name="partner-centrelink">
                                <span>No</span>
                            </label>
                        </div>
                    </div>
                    <!-- [I] Partner payment type -->
                    <div id="partner-payment-type">
                        <label class="uikit-text-input__label margin-above" for="tags">Payment type</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                        <label class="uikit-text-input__label margin-above" for="tags">Amount received per fortnight</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="$0.00" style="max-width: 22em;">
                        <label class="uikit-text-input__label margin-above" for="tags">Customer Reference Number (CRN)</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="" style="max-width: 22em;">
                        <label class="uikit-text-input__label margin-above">Date of application
                            <span class="hint">(dd / mm / yyyy)</span>
                        </label>
                        <input class="uikit-text-input dd" type="text" name="enlistStartDay" maxlength="2" required aria-required="true" aria-label="2 digit day"> /
                        <input class="uikit-text-input mm" type="text" name="enlistStartMonth" maxlength="2" required aria-required="true" aria-label="2 digit month"> /
                        <input class="uikit-text-input yyyy" type="text" name="enlistStartYear" maxlength="4" required aria-required="true" aria-label="4 digit year">
                        <label class="uikit-text-input__label margin-above" for="tags">Regional office</label>
                        <input class="uikit-text-input" name="tags" id="tags" type="text" value="">
                    </div>
                </div>
                <!-- bottom page navigation -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="pagination">
                            <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href='/sp-my-details'">Previous</button>
                            <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href='#open-modal'">Cancel</button>
                            <div class="floated">
                                <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href='/claims'">Save and exit</button>
                                <button class="uikit-btn btnNext" onclick="window.location.href='/sp-dependant'">Save and next</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div><!-- bottom page navigation end -->
            <% include ../partials/cancel-modal.ejs %>
        </div><!-- form-group end -->
    </div> <!-- container end -->
</main>
<% include ../../../global/partials/footer.ejs %>
<script>
$(Document).ready(function() {

    $("#date-marriage").hide();
    $("#date-living-together").hide();
    $("#date-not-together").hide();
    $("#deceased-partner").hide();
    $("#date-separation").hide();
    $("#partner-details").hide();
    $("#partner-details-other").hide();
    $("#partners-home-address").hide();
    $("#partner-payment-type").hide();
    $("#partner-cshc-details").hide();
    $("#divorced-date").hide();


    $("#partner-centrelink-yes").click(function() {
        $("#partner-payment-type").show("fast");
    });

    $("#partner-csh-yes").click(function() {
        $("#partner-cshc-details").show("fast");
    });
    $("#partner-csh-no").click(function() {
        $("#partner-cshc-details").hide("fast");
    });

    $("#single").click(function() {
        hideAll();
        $("#date-marriage").hide("fast");
        $("#date-living-together").hide("fast");
        $("#date-not-together").hide("fast");
        $("#deceased-partner").hide("fast");
        $("#date-separation").hide("fast");
        $("#partner-details").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });

    $("#married").click(function() {
        $("#date-marriage").show("fast");
        $("#partner-details").show("fast");
        $("#date-living-together").hide("fast");
        $("#date-not-together").hide("fast");
        $("#deceased-partner").hide("fast");
        $("#date-separation").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });

    $("#defacto").click(function() {
        $("#date-living-together").show("fast");
        $("#partner-details").show("fast");
        $("#date-marriage").hide("fast");
        $("#date-not-together").hide("fast");
        $("#deceased-partner").hide("fast");
        $("#date-separation").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });

    $("#partner-not-living").click(function() {
        $("#date-not-together").show("fast");
        $("#partner-details").show("fast");
        $("#date-marriage").hide("fast");
        $("#date-living-together").hide("fast");
        $("#deceased-partner").hide("fast");
        $("#date-separation").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });


    $("#divorced").click(function() {
        hideAll();
        $("#deceased-partner").hide("fast");
        $("#date-marriage").hide("fast");
        $("#date-living-together").hide("fast");
        $("#date-not-together").hide("fast");
        $("#date-separation").hide("fast");
        $("#divorced-date").show("fast");
        $("#partner-details").hide("fast");
        $("#partner-details-other").hide("fast");
    });

    $("#widowed").click(function() {
        hideAll();
        $("#deceased-partner").show("fast");
        $("#date-marriage").hide("fast");
        $("#date-living-together").hide("fast");
        $("#date-not-together").hide("fast");
        $("#date-separation").hide("fast");
        $("#partner-details").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });

    $("#married-separated").click(function() {
        hideAll();
        $("#date-marriage").hide("fast");
        $("#date-living-together").hide("fast");
        $("#date-not-together").hide("fast");
        $("#deceased-partner").hide("fast");
        $("#date-separation").show("fast");
        $("#partner-details").hide("fast");
        $("#partner-details-other").hide("fast");
        $("#divorced-date").hide("fast");
    });


    $("#sp-partner-homeAddress-no").click(function() {
        $("#partners-home-address").show("fast");
    });

    $("#sp-partner-homeAddress-yes").click(function() {
        $("#partners-home-address").hide("fast");
    });

    $("#sp-partner-postalAddress-no").click(function() {
        $("#partners-postal-address").show("fast");
    });

    $("#sp-partner-postalAddress-yes").click(function() {
        $("#partners-postal-address").hide("fast");
    });
    hideAll();

    $("#other-names-yes").click(function() {
        $("#sp-partner-formerName").show("fast");
    });
    $("#other-names-no").click(function() {
        $("#sp-partner-formerName").hide("fast");
    });

    $("#partner-centrelink-yes").click(function() {
        $("#partner-payment-type").show("fast");
    });
    $("#partner-centrelink-no").click(function() {
        $("#partner-payment-type").hide("fast");
    });

    var partner = false;
    $(document).keypress(function(e) {
        switch (e.which) {
            case 49:
                partner = false;
                $("#sp-partner-applying").hide();
                $("#partner-centrelink-details").hide("fast");
                break;
            case 50:
                partner = true;
                $("#sp-partner-applying").show();
                $("#partner-centrelink-details").show("fast");
                break;
            default:
        }
    });

    function hideAll() {
        $("#sp-partner-formerName").hide("fast");
        $("#partner-details").hide("fast");
        $("#partners-postal-address").hide("fast");
        $("#sp-partner-applying").hide("fast");
        $("#partner-cshc-details").hide("fast");
        $("#partner-centrelink-details").hide("fast");
        $("#partner-payment-type").hide("fast");
    }
});
</script>
</body>

</html>