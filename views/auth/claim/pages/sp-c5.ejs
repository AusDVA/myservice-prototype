                <!-- Progress bar -->
                <div class="col-md-12">
                    <nav>
                        <ul class="progress-container">
                            <li class="progress-step progress-step--done">
                                <a href="sp-c1">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Savings</p></a>
                            </li>
                            <li class="progress-step progress-step--done">
                                <a href="sp-c2">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Investments</p>
                  </a>
                            </li>
                            <li class="progress-step progress-step--done">
                                <a href="sp-c3">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Income</p>
                  </a>
                            </li>
                            <li class="progress-step progress-step--done">
                                <a href="sp-c4">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Business</p>
                  </a>
                            </li>
                            <li class="progress-step progress-step--active">
                                <a href="sp-c5">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Real estate</p>
                  </a>
                            </li>
                            <li class="progress-step progress-step">
                                <a href="sp-c6">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Other assets</p>
                  </a>
                            </li>
                            <li class="progress-step progress-step--last">
                                <a href="sp-c7">
                    <span class="progress-step__icon"></span>
                    <p class="progress-step-label">Additional circumstances</p>
                  </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- Page heading -->
            <h1 class="margin-above--none ss-align--center">
          <span class="heading">Payments</span>
        </h1>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <!-- Page content -->
                    <h2 class="margin-above--large">
              <span class="font-weight-heavy">Income & payments information</span>
            </h2>
                    <!-- bottom page navigation -->
                    <div class="pagination">
                        <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/sp-c4'">Previous</button>
                        <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '#open-modal'">Cancel claim</button>
                        <div class="floated">
                            <button class="uikit-btn uikit-btn--tertiary" onclick="window.location.href = '/claims'">Save and exit</button>
                            <button class="uikit-btn btnNext" onclick="window.location.href = '/sp-c6'">Save and next</button>
                        </div>
                    </div>
                    <% include ../partials/cancel-modal.ejs %>
                </div> <!-- col-md-12 end -->
            </div> <!-- row end -->
        </div> <!-- container end -->
    </main>
    <% include ../../../global/partials/footer.ejs %>
    <script>
    // Progress percentage
    document.getElementById("percentage ").style.width = "20% ";
    document.getElementById("percentage_value ").textContent = "20% ";
    </script>
