    <script>
    $(Document).ready(function() {

        var vehicleAdded;
        var homeContentsAdded;

        // page navigation
        $("#add-anchor-link-start").click(function() {
            $('html, body').animate({
                scrollTop: $("#add-options").offset().top
            }, 1000);
        });
        $("#add-anchor-link-return").click(function() {
            $('html, body').animate({
                scrollTop: $("#add-options").offset().top
            }, 1000);
        });

        // home contents
        $('#sp-c-home-contents-add').click(function() {
            hideAll();
            $("#home-contents-1-start").show();
            $('html, body').animate({
                scrollTop: $("#user-list").offset().top
            }, 1000);
            homeContentsAdded = true;
        });

    $(function() {
      $("#home-contents-1-start").change(function() {
        if ($("#church-account").is(":selected")) {
         $("#church-account-ref").show("fast");
       } else {
        $("#church-account-ref").hide();
       }
     }).trigger('change');
   });

        $('#home-contents-1-start-add').click(function() {
            hideAll();
            $("#home-contents-1-summary").show();
            $("#add-options").show();
            $("#add-anchor-link-return").show();
            $('html, body').animate({
                scrollTop: $("#user-list").offset().top
