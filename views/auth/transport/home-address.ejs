<%- include(partials+"components/form-partials/dropdown", {
  label: "Select home or enter new",
id: "pickup",
options: [
{
  text: "-- Select address --"
},
{ 
  text: "Unit 3/41 Constitution Ave, Jamison Appartments, Canberra ACT 2612"
},
{
  text: "<strong>Enter new address</strong>"
}
  ]
}); %>


<div class="new-pickup" hidden>
  <div class="form-group margin-below--large" id="address-lookup-container">
    <label for="text470" class="uikit-text-input__label">
      Address<span class="hint margin-above--none margin-below--none"> (start typing your
        address)</span></label>
    <input type="text" name="address" list="address-lookup" />
    <datalist id="address-lookup">
      <option value="334 Heathcliff Rd, Melrose place, NSW 2045">334 Heathcliff Rd, Melrose place, NSW 2045</option>
      <option value="L2 145 Trent St, Salsbury NSW 2034">L2 145 Trent St, Salsbury NSW 2034</option>
      <option value="1245 Dax Crossing Apt. Melrose place, NSW 2897">1245 Dax Crossing Apt. Melrose place, NSW 2897
      </option>
      <option value="Enter address">Enter new address</option>
    </datalist>
  </div>

</div>

<div class="manualinput" hidden>


  <form class="responsive-form margin-below--extra">
    <div class="form-group col-12">
      <label for="text937" class="uikit-text-input__label">
        Building name<span class="hint "> (optional)</span></label>
        <input type="text" class="uikit-text-input">
    </div>
    <div class="form-group col-3">
      <label for="text756" class="uikit-text-input__label">
        Street / unit number</label>
      <input type="text" class="uikit-text-input">
    </div>
    <div class="form-group col-6">
      <label for="text296" class="uikit-text-input__label">
        Street name</label>
      <input type="text" class="uikit-text-input">
    </div>
    <div class="form-group col-3">
      <label for="text793" class="uikit-text-input__label">
        Street type</label>
      <input type="text" class="uikit-text-input">
    </div>
    <div class="form-group col-6">
      <label for="text997" class="uikit-text-input__label">
        Suburb</label>
      <input type="text" class="uikit-text-input">
    </div>
    <div class="form-group formPartials_dropdown col-3">
      <label for="dropdown340" class="uikit-text-input__label">
        State</label>
      <select class="uikit-text-input">
        <option value="selectstate">
          -- Select state --
        </option>
        <option value="act">
          Australian Capital Territory
        </option>
        <option value="nsw">
          New South Wales
        </option>
        <option value="nt">
          Northern Territory
        </option>
        <option value="qld">
          Queensland
        </option>
        <option value="sa">
          South Australia
        </option>
        <option value="tas">
          Tasmania
        </option>
        <option value="vic">
          Victoria
        </option>
        <option value="wa">
          Western Australia
        </option>
      </select>
    </div>
    <div class="form-group col-3">
      <label for="text929" class="uikit-text-input__label">
        Postcode</label>
      <input type="text" class="uikit-text-input">
    </div>
  </form>
</div>
