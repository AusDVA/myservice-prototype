<script>
    // Show and hide content when 'Yes' toggle button is clicked
    $("#yes").click(function () {
      $("#no-content").hide();
      $("#yes-content").show("fast");
    });

    var fewSeconds = 2;

    var btn = $(".save-dd-button, .cancel-dd-button");
    btn.prop('disabled', true);
    $('.waiting-animation').show();
    setTimeout(function () {
      btn.prop('disabled', false);
      // re-enable other edit fields
      $('.edit-text-button, .edit-home-button').prop('disabled', false);
