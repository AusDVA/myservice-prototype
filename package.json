{
	"name": "myservice-prototype",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"scripts": {
		"build": "node-sass src/sass/main.scss docs/css/main.css --output-style compressed",
		"gulp": "gulp",
		"watch:sass": "npm run build && onchange 'src/sass/*.scss' 'tests/docs/*.scss' -- npm run build",
		"serve": "browser-sync start --server --startPath 'docs' --files 'docs/*.html, docs/css/*.css, docs/js/*.js'",
		"watch": "npm-run-all --parallel serve watch:*",
		"test": "echo \"Error: no test specified\" && exit 0",
		"start": "node index.js"
	},
	"browserslist": [
		"last 2 versions",
		"> 1%",
		"ie >= 9"
	],
	"dependencies": {
		"babel-core": "^6.26.3",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"browser-sync": "^2.26.3",
		"cookie-parser": "^1.4.3",
		"ejs": "^2.5.6",
		"express": "^4.16.4",
		"gulp": "^3.9.1",
		"gulp-autoprefixer": "^4.0.0",
		"gulp-babel": "^7.0.1",
		"gulp-jshint": "^2.0.4",
		"gulp-livereload": "^3.8.1",
		"gulp-nodemon": "^2.4.2",
		"gulp-sass": "^3.1.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-uglifycss": "^1.1.0",
		"gulp-util": "^3.0.8",
		"jshint": "^2.9.6",
		"node-sass": "^4.9.2",
		"npm-run-all": "^4.0.2",
		"onchange": "^3.2.1",
		"sass-lint": "^1.12.1",
		"sass-versioning": "^0.2.2",
		"serve-index": "^1.9.1"
	},
	"devDependencies": {},
	"repository": {
		"type": "git",
		"url": "https://github.com/AusDVA/myservice-prototype"
	}
}
