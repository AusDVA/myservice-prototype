{
  "name": "myservice-prototype",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "engines": {
    "node": "8.12.0"
  },
  "scripts": {
    "build": "node-sass src/sass/main.scss docs/css/main.css --output-style compressed",
    "gulp": "gulp",
    "watch:sass": "npm run build && onchange 'src/sass/*.scss' 'tests/docs/*.scss' -- npm run build",
    "serve": "browser-sync start --server --startPath 'docs' --files 'docs/*.html, docs/css/*.css, docs/js/*.js'",
    "watch": "npm-run-all --parallel serve watch:*",
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "node index.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie >= 11"
  ],
  "dependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.7",
    "cookie-parser": "^1.4.4",
    "cssnano": "^4.1.10",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "git-branch": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-changed": "^4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-nodemon": "^2.4.2",
    "gulp-noop": "^1.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "postcss-colorblind": "^1.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-merge-longhand": "^4.0.11",
    "sass-lint": "^1.13.1",
    "sass-versioning": "^0.4.0",
    "serve-index": "^1.9.1",
    "uninstall": "0.0.0",
    "util.env": "0.0.33"
  },
  "devDependencies": {
    "@gov.au/accordion": "^7.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AusDVA/myservice-prototype"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": false,
      "location": "pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "pancake/css/",
      "name": "pancake.min.css"
    },
    "sass": {
      "modules": false,
      "location": "pancake/sass/",
      "name": "pancake.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "pancake/js/",
      "name": "pancake.min.js"
    },
    "react": {
      "location": "pancake/react/"
    }
  }
}
